  3.5776042356450644`*^9}},ExpressionUUID->"808b3faf-c374-40c6-9704-\
6d74d0bb2b28"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"lm", " ", "=", " ", 
    RowBox[{"2", " ", "l"}]}], ";"}], " ", 
  RowBox[{"(*", " ", 
   StyleBox[
    RowBox[{
     RowBox[{"max", " ", "L"}], "-", 
     RowBox[{"value", " ", "in", " ", "the", " ", 
      RowBox[{"expansion", ".", " ", "Rerun"}], " ", "this", " ", "if", " ", 
      "l", " ", "changed"}]}],
    FontColor->RGBColor[0, 0, 1]], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"mxexp", "[", 
    RowBox[{"\[Alpha]_", ",", "\[Beta]_"}], "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"0", "\[LessEqual]", "\[Alpha]", "\[LessEqual]", "\[Beta]"}], 
     ",", "1", ",", "0"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"tmp", " ", "=", " ", "0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "p", ",", "q", ",", "s", ",", "j", ",", "i", ",", "r", ",", "f", ",", 
       "c"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "  ", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"f", " ", "=", " ", "rpot"}], ";", "\[IndentingNewLine]", 
        RowBox[{"c", " ", "=", " ", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"mxexp", "[", 
             RowBox[{
              RowBox[{"p", "+", "q", "+", "s"}], ",", "lm"}], "]"}], 
            "\[Equal]", "1"}], ",", " ", 
           RowBox[{
            RowBox[{"D", "[", 
             RowBox[{
              RowBox[{"D", "[", 
               RowBox[{
                RowBox[{"D", "[", 
                 RowBox[{"f", ",", 
                  RowBox[{"{", 
                   RowBox[{"z", ",", "s"}], "}"}]}], "]"}], ",", 
                RowBox[{"{", 
                 RowBox[{"y", ",", "q"}], "}"}]}], "]"}], ",", 
              RowBox[{"{", 
               RowBox[{"x", ",", "p"}], "}"}]}], "]"}], " ", "/.", " ", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"x", "\[Rule]", "0"}], ",", " ", 
              RowBox[{"y", "\[Rule]", "0"}], ",", 
              RowBox[{"z", "\[Rule]", "0"}]}], "}"}]}], ",", "0"}], "]"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"tmp", " ", "=", " ", 
         RowBox[{"tmp", " ", "+", " ", 
          RowBox[{
           RowBox[{"c", "   ", "/", " ", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"p", "!"}], "  ", 
              RowBox[{"q", "!"}], "  ", 
              RowBox[{"s", "!"}]}], " ", ")"}]}], "  ", 
           SuperscriptBox["x", "p"], " ", 
           SuperscriptBox["y", "q"], " ", 
           SuperscriptBox["z", "s"]}]}]}], " ", ";"}], "\[IndentingNewLine]", 
       ",", 
       RowBox[{"{", 
        RowBox[{"p", ",", "0", ",", "lm"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"q", ",", "0", ",", "lm"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"s", ",", "0", ",", "lm"}], "}"}]}], "]"}]}], "]"}], ";"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"Simplify", "[", 
        RowBox[{"#", " ", ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"a", ">", "0"}], ",", 
           RowBox[{"b", ">", "0"}], ",", 
           RowBox[{"c", ">", "0"}]}], "}"}]}], "]"}], "&"}], " ", "//", " ", 
      "Normal"}], " ", "//", " ", "Expand"}], " ", ";"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"mpot", " ", "=", " ", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      SuperscriptBox["x", "p"], " ", 
      SuperscriptBox["y", "q"], " ", 
      SuperscriptBox["z", "s"], " ", 
      RowBox[{"Coefficient", "[", 
       RowBox[{
        RowBox[{"x", " ", "y", " ", "z", " ", "tmp"}], ",", " ", 
        RowBox[{
         SuperscriptBox["x", 
          RowBox[{"p", "+", "1"}]], " ", 
         SuperscriptBox["y", 
          RowBox[{"q", "+", "1"}]], " ", 
         SuperscriptBox["z", 
          RowBox[{"s", "+", "1"}]]}]}], "]"}], " ", 
      RowBox[{"mxexp", "[", 
       RowBox[{
        RowBox[{"p", "+", "q", "+", "s"}], ",", "lm"}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"p", ",", "0", ",", "lm"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"q", ",", "0", ",", "lm"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"s", ",", "0", ",", "lm"}], "}"}]}], "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.567703179207706*^9, 3.567703273218289*^9}, {
   3.567703327940102*^9, 3.567703391850186*^9}, {3.56770343642141*^9, 
   3.567703520592351*^9}, {3.567703611509699*^9, 3.56770364751054*^9}, {
   3.56770372359675*^9, 3.567703724889741*^9}, {3.56770377418863*^9, 
   3.567703802217478*^9}, {3.567703970356936*^9, 3.567704000532092*^9}, {
   3.567706004428404*^9, 3.567706055279157*^9}, {3.56770707238748*^9, 
   3.567707072777974*^9}, {3.567771942064618*^9, 3.567771943055091*^9}, {
   3.567778014242856*^9, 3.567778019192409*^9}, {3.567867541912158*^9, 
   3.567867566621843*^9}, 3.577514396859861*^9, {3.577526034460178*^9, 
