 RowBox[{
  RowBox[{
   RowBox[{"ftest", " ", "=", " ", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", 
         RowBox[{"4096", " ", "\[Pi]"}]]}], "15", " ", 
       SqrtBox["21"], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"686", " ", 
          RowBox[{"Cos", "[", "t", "]"}]}], "+", 
         RowBox[{"451", " ", 
          RowBox[{"Cos", "[", 
           RowBox[{"3", " ", "t"}], "]"}]}], "+", 
         RowBox[{"143", " ", 
          RowBox[{"Cos", "[", 
           RowBox[{"5", " ", "t"}], "]"}]}]}], ")"}], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"Cos", "[", 
          RowBox[{"2", " ", "p"}], "]"}], "-", 
         RowBox[{"\[ImaginaryI]", " ", 
          RowBox[{"Sin", "[", 
