  RowBox[{
   RowBox[{"rad", "[", 
    RowBox[{"l_", ",", "\[Beta]_"}], "]"}], " ", ":=", " ", 
   RowBox[{
    SqrtBox[
     FractionBox["1", 
      RowBox[{
       SuperscriptBox["2", 
