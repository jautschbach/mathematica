           RowBox[{"tmp", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "1"}], "]"}], "]"}], ",", 
           RowBox[{"tmp", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "2"}], "]"}], "]"}], ",", "t", ",", "p"}], 
          "]"}]}], ",", " ", 
        RowBox[{"{", 
