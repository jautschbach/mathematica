Cell[TextData[{
 "Now we can calculate the coefficients:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["r", "l"], 
     SubsuperscriptBox["Y", "l", "m"]}], "=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["\[Sum]", 
       RowBox[{"p", ",", "q", ",", "s"}]], 
      RowBox[{
       SubsuperscriptBox["C", "pqs", "lm"], 
       SuperscriptBox["x", "p"], 
       SuperscriptBox["y", "q"], 
       SuperscriptBox["z", "s"]}]}], "=", 
     RowBox[{"f", "(", 
      RowBox[{"x", ",", "y", ",", "z"}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"73340806-628b-42e5-9b50-18edb667efea"],
 ", therefore\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["C", "pqs", "lm"], "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", 
       RowBox[{
        RowBox[{"p", "!"}], " ", 
        RowBox[{"q", "!"}], " ", 
        RowBox[{"s", "!"}]}]], 
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox["\[PartialD]", 
         RowBox[{"\[PartialD]", " ", "x"}]], ")"}], "p"], " ", 
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox["\[PartialD]", 
         RowBox[{"\[PartialD]", " ", "y"}]], ")"}], "q"], " ", 
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox["\[PartialD]", 
         RowBox[{"\[PartialD]", " ", "z"}]], ")"}], "s"], "  ", 
      RowBox[{"f", "(", 
       RowBox[{"x", ",", "y", ",", "z"}], ")"}]}], 
     SubscriptBox["|", 
      RowBox[{"x", "=", 
       RowBox[{"y", "=", 
        RowBox[{"z", "=", "0"}]}]}]]}]}], TraditionalForm]],ExpressionUUID->
  "9d598d01-8dee-44cc-b139-566b1b96f975"],
 "\nThe array xyz2ylm is equal to ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["C", "pqs", "lm"], TraditionalForm]],ExpressionUUID->
  "32e53b59-b46a-4404-bd5d-d0bd8f179798"],
 " divided by ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["r", "l"], TraditionalForm]],ExpressionUUID->
  "0fcedd4d-60aa-4558-bbca-5ba426159478"],
 " to get back to the original basis of  ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["Y", "l", "m"], TraditionalForm]],ExpressionUUID->
  "fb8d1116-64a0-47b6-b8c8-72c66c9b957f"],
 "\nTaking derivatives in ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " is fast, therefore there is no point in coding the final result, \nwe can \
let the software do the differentiations. "
}], "Text",
 CellChangeTimes->{{3.567861874250345*^9, 3.567862227668302*^9}, {
  3.567862285622197*^9, 
  3.567862322477766*^9}},ExpressionUUID->"9cce7854-7d17-422e-80a1-\
8bba923f229e"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    StyleBox["xyz2ylm",
     FontColor->RGBColor[1, 0, 0]], " ", "=", " ", 
    RowBox[{"Table", "[", 
     RowBox[{"0", ",", 
      RowBox[{"{", 
       RowBox[{"j", ",", "1", ",", "nb"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "1", ",", "nxyz"}], "}"}]}], "]"}]}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{"initialize", " ", "array"}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{"Block", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
    "p", ",", "q", ",", "s", ",", "j", ",", "i", ",", "r", ",", "f", ",", 
     "c"}], "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{"Do", "[", "  ", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"p", " ", "=", " ", 
       RowBox[{
        RowBox[{"Exponent", "[", 
         RowBox[{
          RowBox[{"x", "  ", 
           RowBox[{"xyzbas", "[", 
            RowBox[{"[", "i", "]"}], "]"}]}], ",", "x"}], "]"}], " ", "-", 
        "1"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"q", " ", "=", " ", 
       RowBox[{
        RowBox[{"Exponent", "[", 
         RowBox[{
          RowBox[{"y", "  ", 
           RowBox[{"xyzbas", "[", 
            RowBox[{"[", "i", "]"}], "]"}]}], ",", "y"}], "]"}], " ", "-", 
        "1"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"s", " ", "=", " ", 
       RowBox[{
        RowBox[{"Exponent", "[", 
         RowBox[{
          RowBox[{"z", "  ", 
           RowBox[{"xyzbas", "[", 
            RowBox[{"[", "i", "]"}], "]"}]}], ",", "z"}], "]"}], " ", "-", 
        "1"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"f", " ", "=", " ", 
       RowBox[{
        RowBox[{"ylmcart", "[", 
         RowBox[{"[", "j", "]"}], "]"}], " ", "//.", " ", 
        RowBox[{"{", 
         RowBox[{"r", " ", "\[Rule]", " ", 
          RowBox[{"Sqrt", "[", 
           RowBox[{
            RowBox[{"x", "^", "2"}], " ", "+", " ", 
            RowBox[{"y", "^", "2"}], " ", "+", " ", 
            RowBox[{"z", "^", "2"}]}], "]"}]}], "}"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"c", " ", "=", " ", 
       RowBox[{
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"D", "[", 
           RowBox[{
            RowBox[{"D", "[", 
             RowBox[{"f", ",", 
              RowBox[{"{", 
               RowBox[{"z", ",", "s"}], "}"}]}], "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"y", ",", "q"}], "}"}]}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", "p"}], "}"}]}], "]"}], " ", "/.", " ", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"x", "\[Rule]", "0"}], ",", " ", 
          RowBox[{"y", "\[Rule]", "0"}], ",", 
          RowBox[{"z", "\[Rule]", "0"}]}], "}"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"xyz2ylm", "[", 
        RowBox[{"[", 
         RowBox[{"j", ",", "i"}], "]"}], "]"}], " ", "=", " ", 
       RowBox[{"c", "   ", "/", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"p", "!"}], "  ", 
          RowBox[{"q", "!"}], "  ", 
          RowBox[{"s", "!"}], " ", 
          SuperscriptBox["r", 
           RowBox[{"(", 
            RowBox[{"p", "+", "q", "+", "s"}], ")"}]]}], ")"}]}]}], " ", 
      ";"}], "\[IndentingNewLine]", ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", "nxyz"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "1", ",", "nb"}], "}"}]}], "]"}]}], "]"}]}], "Input",
